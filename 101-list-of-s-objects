public List<sObject> getListOfsObject(List<Account> accounts, List<Contact> contacts)
{
    // code here
    List<sObject> result = new List<sObject>();
    if ( accounts.size() > 0 ) {

        result.addAll(accounts);

    }
    if ( contacts.size() > 0 ) {

        result.addAll(contacts);

    }

    return result;


}